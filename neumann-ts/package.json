{
  "name": "@scrunchee/client",
  "version": "0.3.0",
  "description": "TypeScript SDK for Neumann database",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "neumann",
    "database",
    "tensor",
    "grpc",
    "vector",
    "graph"
  ],
  "author": "Neumann Team",
  "license": "BSL-1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shadylukin/Neumann.git",
    "directory": "neumann-ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.10"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^8.57.0",
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "grpc-web": "^1.5.0"
  },
  "peerDependenciesMeta": {
    "grpc-web": {
      "optional": true
    }
  }
}
